{
    "name": "Aac/saml2-bearer-server",
    "description": "This is a demo project that implements an Oauth2 server that handles saml2-bearer authorization grants.",
    "license":"MIT",
    "homepage": "https://github.com/krisolafson/saml2-bearer-server",
    "autoload": {
        "psr-4": {"Aac\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Aac\\": "test/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url" : "https://github.com/krisolafson/oauth2-server-php"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "kolafson/oauth2-server-php",
        "onelogin/php-saml": "~2.12",
        "slim/slim": "2.*",
        "slim/views": "*",
        "twig/twig" :"~1.0",
        "monolog/monolog" : "1.12",
        "guzzlehttp/guzzle" : "^5.0"
    }
}
